{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["config/*"],
      "@assets/*": ["assets/*"],
      "@routes/*": ["routes/*"],
      "@models/*": ["models/*"],
      "@middleware*": ["middleware/*"],
      "@controller/*": ["controller/*"],
    }
  },
  "include": ["src"]
}
